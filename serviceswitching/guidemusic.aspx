<script type="text/javascript">
   var AnimatedLetterBoxFeature1Namespace = new Object();
    var curNamespace = AnimatedLetterBoxFeature1Namespace;
    
    //using namespaces in case in the future we want more than one big animated feature.

    curNamespace.imgWait = function(){
        //loops and waits until all images on page are loaded. 
        //this is so that animation doesn't begin with unloaded or partially loaded images
    
        var ready = true;
        var images = document.getElementsByTagName("img");
        
        for(i = 0; i < images.length; i++){
            if(images[i].className == "imageLink" && images[i].complete == false){
                ready = false;
                break;
            }
        }
        
        if(ready){
            if(this.BigFeatures.length > 1){
                this.nextFeature();
            }
        }else{
            setTimeout("AnimatedLetterBoxFeature1Namespace.imgWait()", this.WaitTime);
        }
    }

    curNamespace.nextFeature = function(){
    
        if(!this.moused){
            var cur = this.currentFeature;
            var next = this.getNextFeature();
            
            if(this.curObj.filters){
                this.curObj.filters[0].Duration = this.TransitionTime/1000;
                this.curObj.filters[0].apply();
                this.curObj.innerHTML = this.BigFeatures[next].innerHTML;
                this.curObj.filters[0].play();
            }else{
                this.curObj.innerHTML = this.BigFeatures[next].innerHTML;
            }
            this.currentFeature = next;
            setTimeout("AnimatedLetterBoxFeature1Namespace.nextFeature()", this.WaitTime+this.TransitionTime);
        }else{
            setTimeout("AnimatedLetterBoxFeature1Namespace.nextFeature()", this.WaitTime);
        }
    }
    
    curNamespace.getNextFeature = function(){
        var next = this.currentFeature+1;
        if(next == this.BigFeatures.length) next = 0; //loop
        return next;
    }
    
    //all our features are stored in an Array
    curNamespace.BigFeatures = new Array();
    curNamespace.currentFeature = 0;
    curNamespace.addBigFeature = function(id){
        this.BigFeatures.push(id);
    }
    
   
    curNamespace.WaitTime = 5000; //Time spent "resting" between transitions
    curNamespace.TransitionTime = 1000; //Time spent on each transition
    curNamespace.moused = false;
    
    
</script>

